<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swpuiot.mydrawerlayout2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/winner_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.view.DiaryActivity" />
        <activity android:name=".view.view.ReviseDiaryActivity" />
        <activity android:name=".view.view.ReadDiaryActivity" />
        <activity android:name=".view.view.AboutAPPActivity" />
        <service android:name=".view.services.TimeKeepService"/>
        <receiver android:name=".view.receiver.AlarmReceiver"/>
    </application>

</manifest>
